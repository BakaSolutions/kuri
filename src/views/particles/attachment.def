<a href="{{=it.foxtan.upload}}{{!file.path}}" target="_blank" {{? file.mime.split('/')[0] === "audio" }} onclick="loadAudio(event, '{{=it.foxtan.upload}}{{!file.path}}', '{{!file.name}}')" {{?? ["image", "video"].includes(file.mime.split('/')[0]) }} onclick="media.prepare(event, '{{=file.mime}}', '{{!file.name}}')" {{?}}>
	{{!file.name}} 
	<span>({{!(file.size / 1048576).toFixed(2)}} Mb)</span>
</a>