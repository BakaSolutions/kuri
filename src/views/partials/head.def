<head>
  <meta charset='utf-8'>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{!it.title}}</title>
  <link rel='shortcut icon' href='/{{!it.site.pathPrefix}}favicon.ico'>
  <link rel="stylesheet" href="/{{!it.site.pathPrefix}}css/{{!it.mainStyleshet}}">
  <script id="script" type="text/javascript" src="/{{!it.site.pathPrefix}}js/resquer.min.js"></script>
  <script type="text/javascript">
  window.onload = () => {
    let css = {{=it.dependencies.css}};
    let js = {{=it.dependencies.js}};

    (new RSQ([css, js])).run()
  }
  </script>
  <noscript>
  	<style>
  		.jsOnly{
  			display: none !important
  		}
  		#replyForm input[type="file"] {
  	  	display: block !important;
  		}
  	</style>
  </noscript>
  <style media="screen" id="customTheme"></style>
  <style media="screen" id="configStyle">
  	:root{
  		--border-radius: 0;
  	}
  </style>
</head>
